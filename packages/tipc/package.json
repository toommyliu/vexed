{
  "name": "@vexed/tipc",
  "version": "0.4.0",
  "type": "module",
  "scripts": {
    "postinstall": "tsc && tsup tipc/src/main.ts tipc/src/renderer.ts --format esm,cjs"
  },
  "exports": {
    "./main": {
      "types": "./tipc/main/index.d.ts",
      "require": "./main/index.js",
      "import": "./dist/main.js"
    },
    "./renderer": {
      "types": "./tipc/renderer/index.d.ts",
      "require": "./renderer/index.js",
      "import": "./dist/renderer.js"
    },
    ".": {
      "require": "./dist/main.cjs",
      "import": "./dist/main.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "main": [
        "./tipc/main/index.d.ts"
      ],
      "renderer": [
        "./tipc/renderer/index.d.ts"
      ]
    }
  },
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./tipc/main/index.d.ts",
  "devDependencies": {
    "@lukeed/uuid": "^2.0.1",
    "@tybys/electron-ipc-handle-invoke": "^1.0.2",
    "electron": "^6.1.12",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@lukeed/uuid": "^2.0.1",
    "@tybys/electron-ipc-handle-invoke": "^1.0.2",
    "electron": "^6.1.12"
  }
}
