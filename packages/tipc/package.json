{
  "name": "@vexed/tipc",
  "version": "0.4.0",
  "scripts": {
    "build": "tsup src/main.ts src/renderer.ts --format cjs,esm --outDir dist",
    "postinstall": "pnpm run build"
  },
  "exports": {
    "./main": {
      "types": "./src/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    },
    "./renderer": {
      "types": "./src/renderer.d.ts",
      "import": "./dist/renderer.mjs",
      "require": "./dist/renderer.js"
    },
    ".": {
      "types": "./src/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "main": [
        "./src/main.d.ts"
      ],
      "renderer": [
        "./src/renderer.d.ts"
      ]
    }
  },
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./src/main.d.ts",
  "devDependencies": {
    "@lukeed/uuid": "^2.0.1",
    "@swc/core": "^1.14.0",
    "electron": "workspace:*",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@lukeed/uuid": "^2.0.1",
    "electron": "workspace:*"
  }
}
