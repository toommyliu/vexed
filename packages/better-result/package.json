{
  "name": "better-result",
  "version": "2.5.1",
  "private": true,
  "scripts": {
    "build": "tsdown --outDir ./dist --format cjs --entry './src/index.ts' --dts --exports",
    "sync": "node scripts/sync.mjs",
    "postinstall": "npm run sync || echo 'better-result sync failed, using existing build'"
  },
  "devDependencies": {
    "tsdown": "0.20.0-beta.3",
    "typescript": "^5.9.3"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": "./dist/index.cjs",
    "./package.json": "./package.json"
  }
}