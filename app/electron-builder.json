{
  "productName": "vexed",
  "electronVersion": "6.1.12",
  "directories": {
    "output": "../../build"
  },
  "mac": {
    "files": ["dist/**/*", "../../assets/**/*", "!../../assets/pepflashplayer.dll"],
    "asar": "false",
    "target": {
      "target": "zip",
      "arch": ["x64"]
    },
    "icon": "../../assets/icon.png",
    "artifactName": "${productName}-${os}.${ext}"
  },
  "win": {
    "files": ["dist/**/*", "../../assets/**/*", "!../../assets/PepperFlashPlayer.plugin"],
    "icon": "../../assets/icon.ico",
    "asar": "false",
    "target": {
      "target": "zip",
      "arch": ["x64"]
    },
    "artifactName": "${productName}-${os}.${ext}"
  },
  "extraMetadata": {
    "name": "vexed",
    "main": "./dist/main/index.js"
  }
}
